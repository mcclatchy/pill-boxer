{
  "env": {
    "browser": true,
    "node": true,
    "es2017": true
  },
  "globals": {
    "qs": true,
    "qsa": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "no-console": ["warn", { "allow": ["warn", "error"] }],
    "comma-dangle": 0,
    "semi": [0, "always"],
    "no-unused-vars": 1
  }
}
